{
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(img/.*)",
      "dest": "/dist/img/$1"
    },
    {
      "src": "/(.*)\\.(js|css|html|ico|json)",
      "dest": "/dist/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html",
      "status": 200
    }
  ]
}

